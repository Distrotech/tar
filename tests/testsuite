echo dwa > temp
echo trzy >> temp
cat temp | tr '\\n' '\\0' > temp1
echo jeden > file-list
cat temp1 >> file-list

genfile -f \"jeden
dwa\" || exit 77
genfile -f trzy

tar cfTv archive file-list | sort
)"
at_fn_check_prepare_notrace 'a `...` command substitution' "T-null.at:28"
